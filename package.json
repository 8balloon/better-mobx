{
  "name": "better-mobx",
  "version": "0.0.0",
  "description": "typesafe e2e stream processing",
  "author": "Ethan Gregory Clark <ethangclark@gmail.com>",
  "repository": "https://github.com/8balloon/better-mobx",
  "license": "MIT",
  "keywords": [
    "streaming",
    "typesafe",
    "materialize",
    "reactive",
    "live"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "browser": "./dist/esbuild/browser.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:note": "see https://jestjs.io/docs/troubleshooting",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-browser",
    "esbuild-browser": "esbuild src/browser.tsx --bundle --minify --sourcemap --outfile=dist/esbuild/browser.js",
    "esbuild-browser:dev": "esbuild src/browser.tsx --bundle --sourcemap --outfile=dist/esbuild/browser.js",
    "esbuild-browser:watch": "esbuild src/browser.tsx --bundle --sourcemap --watch --outfile=dist/esbuild/browser.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "mobx": "^6.3.10",
    "mobx-react-lite": "^3.2.2",
    "redux": "^4.1.2"
  }
}
