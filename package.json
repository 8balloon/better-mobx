{
  "name": "r2v",
  "version": "1.0.0",
  "description": "typesafe Vue-like React state management",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "files": [
    "dist/",
    "types/"
  ],
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/eslint": "^8.44.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint \"*/**/*.{ts,js,json}\"",
    "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "build": "rollup --config ./rollup.config.mjs",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "dev": "rollup --config ./rollup.dev.config.mjs --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethangclark/r2v.git"
  },
  "keywords": [],
  "author": {
    "name": "Ethan Gregory Clark"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ethangclark/r2v/issues"
  },
  "homepage": "https://github.com/ethangclark/r2v#readme",
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-replace": "^5.0.3",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "mobx-utils": "^6.0.8",
    "type-fest": "^4.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
